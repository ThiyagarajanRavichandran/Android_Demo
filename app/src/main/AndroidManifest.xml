<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thiyagarajan.android_demo">

    <uses-sdk
        android:maxSdkVersion="23"
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main_Android"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.toolbar.ToolBar_Demo"
            android:label="Tool_Bar" />
        <activity
            android:name="com.example.intent.Intent_Demo"
            android:label="@string/title_activity_intent__demo" />
        <activity
            android:name="com.example.toolbar.Menu_Next"
            android:label="@string/title_activity_menu__next">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.toolbar.ToolBar_Demo" />
        </activity>
    </application>

</manifest>
